  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug wlog",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "env": {
        "GOPATH": "/Users/tal/.gvm/pkgsets/go1.22.7/global",
        "GOROOT": "/Users/tal/.gvm/gos/go1.22.7",
        "PATH": "/Users/tal/.gvm/gos/go1.22.7/bin:${env:PATH}"
