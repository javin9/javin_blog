      "mode": "auto",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/main.go",
      "dlvFlags": ["--check-go-version=false"],
      "showLog": true,
      "env": {
        "GOPATH": "/Users/tal/go",
        "GOROOT": "/Users/tal/go/go1.21.4",
        "PATH": "/Users/tal/go/go1.21.4/bin:/Users/tal/go/bin:${env:PATH}"
      },
      "dlvLoadConfig": {
        "followPointers": true,
        "maxVariableRecurse": 1,
        "maxStringLen": 512,
        "maxArrayValues": 64,
        "maxStructFields": -1
      }
    }
  ]
